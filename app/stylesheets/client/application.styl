@media screen and (max-width: 480px)
  body
    .header ul, .header li, .section li, .header p, .header ol, .section p, .section ul, .section ol, .section cite
      font-size: 13px
      line-height: auto
  pre code
    font-size: 10px;
    line-height: auto
  content
    margin-left: 0px
    
h1, h2, h3, h4
  -webkit-font-smoothing: antialiased
    
p, li
  font-size: 16px !important
  line-height: 1.5 !important
  // remove the next 3 lines to convert back to normal font
  line-height: 1.4 !important
  font-family: 'Forum', cursive !important;
  font-size: 17px !important

pre
  white-space: pre !important
  overflow-x: auto !important
  .str, .atv
